    <rdfs:seeAlso rdf:resource="https://go.drugbank.com/drugs/DB01065"/>
  </rdf:Description>

  <rdf:Description rdf:about="http://cronobiologia.um.es/recurso/Farmaco_Ramelteon">
    <rdf:type rdf:resource="https://w3id.org/biolink/vocab/Drug"/>
