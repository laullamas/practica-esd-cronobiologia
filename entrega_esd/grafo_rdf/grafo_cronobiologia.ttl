    rdfs:comment "CRSD caracterizado por múltiples episodios de sueño fragmentado distribuidos en 24h sin patrón claro."@es ;
    rdfs:seeAlso mondo:0024379 ;
    crono_o:tieneFenotipo crono:Fenotipo_DesincronizacionCircadiana ;
    crono_o:tieneFenotipo crono:Fenotipo_PatronSuenoAlterado ;
    crono_o:tieneFenotipo crono:Fenotipo_DificultadConciliar ;
    crono_o:tieneFenotipo crono:Fenotipo_DificultadMantener ;
    crono_o:tieneFenotipo crono:Fenotipo_SuenoFragmentado ;
    crono_o:tieneFenotipo crono:Fenotipo_DespertarTemprano ;
    crono_o:tieneFenotipo crono:Fenotipo_CalidadPobreSueno ;
    crono_o:tieneFenotipo crono:Fenotipo_SomnolenciaDiurna ;
    crono_o:tratadoCon crono:Farmaco_Melatonina .

# 4. Non-24-Hour Sleep-Wake Disorder
crono:Trastorno_Non24 rdf:type biolink:Disease ;
