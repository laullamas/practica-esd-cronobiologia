    crono_o:tieneFenotipo crono:Fenotipo_DificultadMantener ;
    crono_o:tieneFenotipo crono:Fenotipo_SuenoFragmentado ;
    crono_o:tieneFenotipo crono:Fenotipo_DespertarTardio ;
    crono_o:tieneFenotipo crono:Fenotipo_CalidadPobreSueno ;
    crono_o:tieneFenotipo crono:Fenotipo_SomnolenciaDiurna ;
    crono_o:tratadoCon crono:Farmaco_Tasimelteon .

